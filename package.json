{
  "name": "advancedreact2019",
  "version": "1.0.0",
  "description": "advanced React",
  "main": "index.js",
  "scripts": {
    "start": "build",
    "dev": "webpack-dev-server --history-api-fallback",
    "build": "webpack",
    "lint": "standard",
    "test": "jest --watch",
    "test:coverage": "jest --coverage --colors"
  },
  "repository": {
    "type": "git",
